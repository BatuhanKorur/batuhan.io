.f-center {
    @apply flex items-center justify-center;
}

.f-between {
    @apply flex items-center justify-between;
}

.t-200 {
    @apply transition duration-200 ease-in-out;
}

.page-enter-active,
.page-leave-active {
    transition: all 0.2s;
}

.page-enter-from,
.page-leave-to {
    opacity: 0;
    filter: blur(1rem);
}